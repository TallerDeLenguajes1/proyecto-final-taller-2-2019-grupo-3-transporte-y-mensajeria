<Page x:Class="UI_WPF.Vistas.VistasIncio.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UI_WPF.Vistas.VistasIncio"
      mc:Ignorable="d" 
      d:DesignHeight="610" d:DesignWidth="792"
      Title="HomePage">
    <Grid>
        <Grid >

            <Border BorderThickness="1" Margin="20,80,22,21" CornerRadius="10" Background="White">
                <Grid>
                    <Grid Margin="10">
                        <ScrollViewer VerticalScrollBarVisibility="Hidden" ScrollViewer.CanContentScroll="True">
                            <Frame x:Name="frmHomeContent"/>
                        </ScrollViewer>
                    </Grid>
                    
                </Grid>
            </Border>
            <TextBlock HorizontalAlignment="Left" Margin="20,37,0,0" TextWrapping="Wrap" Text="INICIO" VerticalAlignment="Top" FontFamily="Myriad Pro" FontWeight="Bold" FontSize="25"/>
        </Grid>
    </Grid>
</Page>
